<!--pages/home/home.wxml-->
<view>
 <image class="top-theme" src="{{themeA.entrance_img}}"></image>
 <swiper class="swiper" 
  indicator-dots="{{true}}" 
  indicator-active-color="#157658" 
  autoplay="{{true}}" 
  interval="{{5000}}" 
  duration="{{500}}"
  circular="{{true}}">
     <block wx:for="{{bannerB.items}}">
         <swiper-item>
             <image class="swiper" src="{{item.img}}" />
         </swiper-item>
     </block>
 </swiper>
</view>
